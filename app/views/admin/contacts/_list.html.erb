<table class="table">
  <tr>
    <th>チェック</th>
    <th>投稿者</th>
    <th>ご用件</th>
    <th>内容</th>
  </tr>
  <% contacts.each do |contact| %>
  <tr>
    <td id="check_btn_<%= contact.id %>">
      <%= render 'admin/contacts/check', contact: contact %>
    </td>
    <td><%= contact.name %></td>
    <td><%= contact.subject_i18n %></td>
    <td>
      <%= link_to admin_contact_path(contact) do %>
        <%= contact.message.truncate(10) %>
      <% end %>
    </td>
  </tr>
  <% end %>
</table>