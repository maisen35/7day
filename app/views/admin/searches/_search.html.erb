<br>
<div class="search_form">
  <%= form_with url: admin_search_path, local: true, method: :get do |f| %>
    <div class="d-flex justify-content-end">
      <%= f.text_field :words, class: 'form-control col-md-6 col-sm-10' %>
    </div>
    <div class="d-flex justify-content-end">
      <%= f.select :range, options_for_select([['ユーザー','User'], ['投稿','Tweet'], ['お問い合わせ','Contact']]), {}, class: 'form-control col-md-3 col-sm-4' %>
      <%= f.submit "検索", class: "btn btn-primary" %>
    </div>
  <% end %>
</div>
<br>