<div class="container">
  <div class="row">
    <div class="col">
      <table class="table">
        <tr>
          <th>名前</th>
          <th>メールアドレス</th>
          <th>会員ステータス</th>
        </tr>
        <% @users.each do |user| %>
          <tr>
            <td>
              <%= link_to admin_user_path(user) do %>
                <%= user.name %>
              <% end %>
            </td>
            <td><%= user.email %></td>
            <td><%= user.is_active %></td>
          </tr>
        <% end %>
      </table>
    </div>
  </div>
</div>