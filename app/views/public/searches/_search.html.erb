<br>
<div class="search_form d-flex justify-content-end">
  <%= form_with url: search_path, local: true, method: :get do |f| %>
    <%= f.text_field :words %>
    <%= f.select :range, options_for_select([['User'], ['Tweet']]) %>
    <%= f.submit "検索", class: "btn btn-primary" %>
  <% end %>
</div>
